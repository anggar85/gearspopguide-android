                       android:layout_marginTop="2dp"
                       app:layout_constraintBottom_toTopOf="@+id/framelayout_image6"
                       app:layout_constraintEnd_toStartOf="@+id/framelayout_image3"
                       app:layout_constraintHorizontal_bias="0.5"
                       app:layout_constraintStart_toEndOf="@+id/framelayout_image1"
                       app:layout_constraintTop_toBottomOf="@+id/editText_deckName">

                       <ImageView
                           android:id="@+id/imageViewDeck2"
                           android:layout_width="wrap_content"
                           android:layout_height="wrap_content"
                           android:src="@drawable/gears" />
                   </FrameLayout>

                   <FrameLayout
                       android:id="@+id/framelayout_image3"
                       android:layout_width="70dp"
                       android:layout_height="70dp"
                       android:layout_marginTop="2dp"
                       app:layout_constraintBottom_toTopOf="@+id/framelayout_image7"
                       app:layout_constraintEnd_toStartOf="@+id/framelayout_image4"
                       app:layout_constraintHorizontal_bias="0.5"
                       app:layout_constraintStart_toEndOf="@+id/framelayout_image2"
                       app:layout_constraintTop_toBottomOf="@+id/editText_deckName">

                       <ImageView
                           android:id="@+id/imageViewDeck3"
                           android:layout_width="wrap_content"
                           android:layout_height="wrap_content"
                           android:src="@drawable/gears" />
                   </FrameLayout>

                   <FrameLayout
                       android:id="@+id/framelayout_image5"
                       android:layout_width="70dp"
                       android:layout_height="70dp"
                       android:layout_marginBottom="8dp"
                       app:layout_constraintBottom_toBottomOf="parent"
                       app:layout_constraintEnd_toStartOf="@+id/framelayout_image6"
                       app:layout_constraintHorizontal_bias="0.5"
                       app:layout_constraintStart_toStartOf="parent"
                       app:layout_constraintTop_toBottomOf="@+id/framelayout_image1">

                       <ImageView
                           android:id="@+id/imageViewDeck5"
                           android:layout_width="wrap_content"
                           android:layout_height="wrap_content"
                           android:src="@drawable/gears" />
                   </FrameLayout>
               </androidx.constraintlayout.widget.ConstraintLayout>
           </androidx.cardview.widget.CardView>

           <androidx.cardview.widget.CardView
               android:id="@+id/cardView_recycler"
               android:layout_width="0dp"
               android:layout_height="wrap_content"
               android:layout_marginStart="16dp"
               android:layout_marginEnd="16dp"
               app:layout_constraintEnd_toEndOf="parent"
               app:layout_constraintStart_toStartOf="parent"
               app:layout_constraintTop_toBottomOf="@+id/cardView2">

               <androidx.constraintlayout.widget.ConstraintLayout
                   android:layout_width="match_parent"
                   android:layout_height="match_parent">

                   <androidx.recyclerview.widget.RecyclerView
                       android:id="@+id/recycler_pinListDeck"
                       android:layout_width="match_parent"
                       android:layout_height="200dp"
                       android:layout_marginBottom="8dp"
                       app:layout_constraintBottom_toBottomOf="parent"
                       app:layout_constraintEnd_toEndOf="parent"
                       app:layout_constraintStart_toStartOf="parent"
                       app:layout_constraintTop_toTopOf="parent" />
               </androidx.constraintlayout.widget.ConstraintLayout>
           </androidx.cardview.widget.CardView>


       </androidx.constraintlayout.widget.ConstraintLayout>


    </androidx.core.widget.NestedScrollView>

    <com.google.android.gms.ads.AdView
